{
  "name": "subash-portfolio-monorepo",
  "version": "1.0.0",
  "description": "Full-stack portfolio website with React frontend and Express backend",
  "private": true,
  "type": "module",
  "homepage": "https://subash-s-66.github.io/Subash-Portfolio",
  "scripts": {
    "build": "cd frontend && node ../node_modules/vite/bin/vite.js build && xcopy /E /I /Y dist ..\\backend\\dist",
    "start": "cd backend && node server.js",
    "dev": "cd frontend && node ../node_modules/vite/bin/vite.js",
    "preview": "cd frontend && node ../node_modules/vite/bin/vite.js preview",
    "server": "cd backend && nodemon server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\""
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "framer-motion": "^11.18.2",
    "gsap": "^3.14.2",
    "helmet": "^7.1.0",
    "lenis": "^1.3.17",
    "lucide-react": "^0.309.0",
    "nodemailer": "^6.9.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.5.2",
    "resend": "^6.1.0",
    "three": "^0.160.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "terser": "^5.44.0",
    "vite": "^5.4.20"
  },
  "keywords": [
    "portfolio",
    "react",
    "express",
    "fullstack",
    "monorepo"
  ],
  "author": "Subash S",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
